{
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
    }
  },
  "plugins": [
  ],
  "rules": {
    "quotes": [2, "single", "avoid-escape"],
    "semi": [2, "always"],
    "camelcase": [2, {"properties": "never"}],
    "strict": [2, "global"],
    "eqeqeq": [2, "smart"],
    "curly": [2, "all"],
    "no-spaced-func": 2,
    "no-unused-vars": 2,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 2
  }
}
